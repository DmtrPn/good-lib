{
  "name": "dobro-server",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "dist",
  "types": "dist",
  "scripts": {
    "build": "yarn clean:dist && ./node_modules/.bin/tsc",
    "build:dev": "yarn clean:dist && yarn prettier:fix && yarn lint && ./node_modules/.bin/tsc  --watch",
    "prettier:ceck": "yarn prettier --check src",
    "prettier:fix": "yarn prettier --write src",
    "checkTypes": "./node_modules/.bin/tsc --noEmit",
    "clean:dist": "rm -rf dist",
    "lint": "eslint . --ext .js,.ts ",
    "lint:fix": "eslint . --ext .js,.ts  --fix",
    "test:u": "NODE_ENV=mock jest --detectOpenHandles .*.test.ts",
    "test": "yarn test:u"
  },
  "dependencies": {
    "@nestjs/common": "9.3.9",
    "@nestjs/event-emitter": "1.4.1",
    "chance": "1.1.11",
    "date-fns": "2.29.3",
    "lodash": "4.17.21",
    "log4js": "6.9.1",
    "rxjs": "7.8.0",
    "typeorm": "0.3.12",
    "typeorm-naming-strategies": "4.1.0",
    "typeorm-transactional-cls-hooked": "0.1.21",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/chance": "1.1.3",
    "@types/jest": "29.4.0",
    "@types/lodash": "4.14.191",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "eslint": "8.35.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.7.0",
    "jest": "29.5.0",
    "prettier": "2.8.4",
    "typescript": "5.0.4"
  }
}
